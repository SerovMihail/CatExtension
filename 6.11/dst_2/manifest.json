{
  "version": "0.3",
  "name": "Overwatch FullHD New Tab Wallpapers",
  "manifest_version": 2,
  "description": "Get a lot of FullHD Wallpapers Overwatch for chrome new tabs",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/start/index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "chromedhnewtab",
      "name": "chromedhnewtab",
      "favicon_url": "http://newtabwallpaperstheme.com/favicon.ico",
      "search_url": "https://google.com/search?q={searchTerms}",
      "is_default": true,      
      "encoding": "UTF-8"
    }
  },
  "browser_action": {
    "default_icon": {
      "128": "/start/static/icons/main/128.png"
    }
  },
  "icons": {
    "128": "/start/static/icons/main/128.png"
  },
  "web_accessible_resources": [
    "*"
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "/start/scripts/libs/jquery.min.js",
      "/start/scripts/bg/init/search-engines.js",
      "/start/scripts/bg/init/settings.js",
      "/start/scripts/bg/init/prefs-sys.js",
      "/start/scripts/bg/init/checkTour.js",
      "/start/scripts/bg/utils/index.js",
      "/start/scripts/bg/helpers/common.js",
      "/start/scripts/bg/helpers/setup.js",
      "/start/scripts/bg/helpers/analytics.js"
    ]
  },
  "permissions": [    
    "activeTab",
    "storage",
    "cookies",
    "management",
    "topSites",
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/"
  ]
}