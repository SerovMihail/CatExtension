{
  "version": "0.3",
  "name": "Overwatch FullHD New Tab Wallpapers",
  "description": "Get a lot of FullHD Wallpapers Overwatch for chrome new tabs",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/start/index.html"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "keyword": "chromedhnewtab",
      "name": "chromedhnewtab",
      "favicon_url": "http://newtabwallpaperstheme.com/favicon.ico",
      "search_url": "https://google.com/search?q={searchTerms}",
      "is_default": true,      
      "encoding": "UTF-8"
    }
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/start/libs/jq.min.js",
      "/start/bg/init/search-engines.js",
      "/start/bg/init/settings.js",
      "/start/bg/init/prefs-sys.js",
      "/start/bg/init/checkTour.js",
      "/start/bg/methodsExtend/utils.js",
      "/start/bg/handlers/common.js",
      "/start/bg/handlers/setup.js",
      "/start/bg/handlers/analytics.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/start/static/icons/main/128.png"
    }
  },
  "icons": {
    "128": "/start/static/icons/main/128.png"
  },
  "permissions": [
    "*://mail.google.com/*",
    "activeTab",
    "storage",    
    "cookies",    
    "management",
    "topSites",
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "web_accessible_resources": [ "*" ],
  "manifest_version": 2
}
