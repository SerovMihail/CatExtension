{
  "version": "0.1",
  "name": "Overwatch FullHD New Tab Wallpapers",
  "description": "Get a lot of FullHD Wallpapers Overwatch for chrome new tabs",
  "default_locale": "en",
  "offline_enabled": true,
  "chrome_url_overrides": {
    "newtab": "/start/index.html"
  },
  "background": {
    "persistent": true,
    "scripts": [
      "/start/libs/jquery-2.1.1.min.js",
      "/start/backgroundScripts/search-engines.js",
      "/start/backgroundScripts/settings.js",
      "/start/backgroundScripts/prefs-sys.js",
      "/start/backgroundScripts/utils.js",
      "/start/backgroundScripts/common.js",
      "/start/backgroundScripts/setup.js",
      "/start/backgroundScripts/analytics.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "128": "/start/static/icons/defaultIcons/128.png"
    }
  },
  "icons": {
    "128": "/start/static/icons/defaultIcons/128.png"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",    
    "cookies",    
    "management",
    "topSites"
  ],
  "web_accessible_resources": [ "*" ],
  "manifest_version": 2
}
